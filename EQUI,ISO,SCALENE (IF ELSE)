#include<stdio.h>
int main()
{
	int s1,s2,s3;
	printf("\n enter the 3 sides of a triangle :");
	scanf("%d%d%d",&s1,&s2,&s3);
	if(s1==s2 && s2==s3)
	{
	printf("\n the triangle is equilateral");
	}
	else if(s1==s2 || s2==s3 || s1==s3)
	{
	printf("\n the triangle is isosceles");
	}
	else
	{
	 printf("\n the triangle is scalene");
	}
	return 0;
}
	
